<%= simple_form_for contact, data: { controller: "flatpickr" } do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name, data: { dashlanecreated: "false" } %>
    <%= f.input :birthday, as: :string, input_html: { data: { flatpickr_target: "selectDate" } } %>
    <%= f.input :email %>
    <%= f.input :phone_number %>
    
    <%= f.submit class: "button-success" %>
<% end %>
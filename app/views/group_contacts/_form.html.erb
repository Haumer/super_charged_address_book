<%= simple_form_for [contact, group_contact] do |f| %>
    <div class="group-contact-form d-flex">
        <%= f.association :group, collection: current_user.groups, label: false %>
        <%= button_tag type: 'submit', class: "button-secondary button-connect-left" do %>
            <i class="fa-solid fa-check"></i>
        <% end %>
    </div>
<% end %>
